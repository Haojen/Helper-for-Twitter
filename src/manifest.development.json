{
    "manifest_version": 3,
    "name": "Twtter-Green-Plugs",
    "description": "chrome extension",
    "version": "0.1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        {
            "resources": ["block.svg"],
            "matches": ["*://twitter.com/*"]
        }
    ]
}